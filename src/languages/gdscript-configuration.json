{
  "comments": {
      "lineComment": "#",
      "blockComment": ["\"\"\"", "\"\"\""]
  },
  "brackets": [
      ["(", ")"],
      ["[", "]"],
      ["{", "}"]
  ],
  "autoClosingPairs": [
      ["'", "'"],
      ["\"", "\""],
      ["(", ")"],
      ["[", "]"],
      ["{", "}"]
  ],
  "surroundingPairs": [
      ["'", "'"],
      ["\"", "\""],
      ["(", ")"],
      ["[", "]"],
      ["{", "}"]
  ],
  "indentationRules": {
      "increaseIndentPattern": "^\\s*((class|func|else|elif|for|if|match|while|enum)|(.*\\sdo\\b))\\b[^\\{;]*$",
      "decreaseIndentPattern": "^\\s*([}\\]]([,)]?\\s*(#|$)|\\.[a-zA-Z_]\\w*\\b)|(else|elif)\\b)"
  },
  "folding": {
  "offSide": true,
  "markers": {
    "start": "^\\s*#\\s*region\\b",
    "end": "^\\s*#\\s*endregion\\b"
  }
}
}
